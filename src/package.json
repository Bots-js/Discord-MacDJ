{
  "name": "Discord-MacDJ",
  "productName": "MacDJ",
  "description": "MacDJ, The music bot for Discord.",
  "version": "0.8.0",
  "author": "The DaVinci Team",
  "license": "MIT",
  "homepage": "https://github.com/Bots-js/Discord-MacDJ",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bots-js/Discord-MacDJ.git"
  },
  "bugs": {
    "url": "https://github.com/Bots-js/Discord-MacDJ/issues"
  },
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "electron index.js",
    "pack:osx": "./node_modules/.bin/electron-packager . $npm_package_productName --app-version=$npm_package_version --version=1.3.1 --out=../builds --ignore='(/bin/linux|/bin/win32)' --prune --platform=darwin --arch=x64 --version-string.CompanyName='The DaVinci Team' --version-string.LegalCopyright='Copyright (C) 2016 The DaVinci Team' --version-string.FileDescription=$npm_package_productName --version-string.InternalName=$npm_package_productName --version-string.ProductName=$npm_package_productName --version-string.ProductVersion=$npm_package_version --asar=true --icon=../macdj.icns --overwrite",
    "pack:win32": "./node_modules/.bin/electron-packager . $npm_package_productName --app-version=$npm_package_version --version=1.3.1 --out=../builds --ignore='(/bin/linux|/bin/darwin)' --prune --platform=win32 --arch=ia32 --version-string.CompanyName='The DaVinci Team' --version-string.LegalCopyright='Copyright (C) 2016 The DaVinci Team' --version-string.FileDescription=$npm_package_productName --version-string.OriginalFilename='MacDJ.exe' --version-string.InternalName=$npm_package_productName --version-string.ProductName=$npm_package_productName --version-string.ProductVersion=$npm_package_version --asar=true --icon=../macdj.ico --overwrite",
    "pack:win64": "./node_modules/.bin/electron-packager . $npm_package_productName --app-version=$npm_package_version --version=1.3.1 --out=../builds --ignore='(/bin/linux|/bin/darwin)' --prune --platform=win32 --arch=x64 --version-string.CompanyName='The DaVinci Team' --version-string.LegalCopyright='Copyright (C) 2016 The DaVinci Team' --version-string.FileDescription=$npm_package_productName --version-string.OriginalFilename='MacDJ.exe' --version-string.InternalName=$npm_package_productName --version-string.ProductName=$npm_package_productName --version-string.ProductVersion=$npm_package_version --asar=true --icon=../macdj.ico --overwrite"
  },
  "dependencies": {
    "discordie": "github:Bots-js/discordie#macdj",
    "ytdl-core": "^0.7.17"
  },
  "devDependencies": {
    "electron-packager": "^7.4.0",
    "electron-prebuilt": "^1.2.0",
    "grunt": "^1.0.1",
    "grunt-appdmg": "^0.4.0",
    "grunt-electron": "^4.0.0",
    "gruntify-eslint": "^3.0.0",
    "load-grunt-tasks": "^3.5.0"
  }
}
